{
    "compilerOptions": {
        "module": "commonjs",
        "allowJs": true,
        "lib": [
            "DOM",
            "ES2021"
        ],
        "strictNullChecks": false,
        "skipLibCheck": true,
        "pretty": true,
        "suppressImplicitAnyIndexErrors": true,
        "noImplicitAny": false,
        "target": "ES2021"
    },
    "exclude": [
        "node_modules"
    ]
}